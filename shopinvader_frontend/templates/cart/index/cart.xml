<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <template id="section_cart" name="Shopinvader cart content">
    
    <h3 t-if="request.debug"><t t-esc="cart_step.name"/></h3>

    <!-- This is not like the liquid templates -->
    <!-- TODO: if we need to render different templates let's call other templates dinamically w/ #{tmpl} -->
    <t t-if="cart_step.key=='cart_index'">
      <t t-set="checkout_button" t-value="False"/>
      <t t-set="cart" t-value="store['cart']"/>

      <div class="container" id="cart" data-shopinvader-container="">

        <p>Here is the content of your cart</p>

        <!-- <div class="col-12 col-md-12 col-lg-8 block block_cart_lines"> -->
          <div class="col-12 col-md-12 block block_cart_lines">

            <t t-call="shopinvader_frontend.block_cart_lines">
              <t t-set="readonly" t-value="False"/>
            </t>
            <!-- This is custom need to be updated with shopinvader example -->
            <!-- <div class="row border-bottom"> -->
              <!--   <div class="col-6">Product</div> -->
              <!--   <div class="col-1">Qty</div> -->
              <!--   <div class="col-2">Unit price</div> -->
              <!--   <div class="col-1">Tax</div> -->
              <!--   <div class="col-2">Total</div> -->
              <!-- </div> -->
            <!-- <t t-foreach="store['cart']['lines']['items']" t-as="item"> -->
              <!-- <div class="row"> -->
                <!--   <div class="col-6"> -->
                  <!--     <t t-esc="item['product']['name']"/> -->
                  <!--   </div> -->
                <!--   <div class="col-1"> -->
                  <!--     <t t-esc="item['qty']"/> -->
                  <!--   </div> -->
                <!--   <div class="col-2"> -->
                  <!--     <t t-esc="item['amount']['price']"/> -->
                  <!--   </div> -->
                <!--   <div class="col-1"> -->
                  <!--     <t t-esc="item['amount']['tax']"/> -->
                  <!--   </div> -->
                <!--   <div class="col-2"> -->
                  <!--     <t t-esc="item['amount']['total']"/> -->
                  <!--   </div> -->
                <!-- </div> -->
              <!-- </t> -->

          </div>
          <div class="col-12 col-md-6 col-lg-4 block block_cart_summary">

            <t t-call="shopinvader_frontend.block_cart_summary">
              <t t-set="display_lines" t-value="False"/>
              <t t-set="submit_label" t-value="Continue"/>
              <t t-set="display_submit" t-value="True"/>
            </t>


          </div>

        </div>
      </t>
      <t t-elif="cart_step.key=='cart_login'">
        <t t-call="shopinvader_frontend.block_cart_login">
        </t>
      </t>
      <t t-elif="cart_step.key=='cart_address'">
        <!-- <p>Let s get the address rocking</p> -->
        <t t-call="shopinvader_frontend.page_cart_address"/>
        <!-- <t t-call="shopinvader_frontend.block_cart_login"> -->
        <!-- </t> -->
      </t>


  </template>

</odoo>

