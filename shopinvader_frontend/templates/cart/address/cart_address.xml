<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <template id="cart_address" name="Shopinvader cart shipping">

    <t t-call="shopinvader_frontend.default_layout">
      <div class="page">
        <div class="container-fluid">
          <div class="row">
            <div class="col-12">
              <h3>My shipping address</h3>
            </div>
            <div class="col-8">
              <!-- <t t-if="'item_id' in request.httprequest.args" t-call="shopinvader_frontend.block_cart_address_list"/> -->
              <!-- <t t-else="" t-call="shopinvader_frontend.block_cart_address_list"/> -->
              <t t-call="shopinvader_frontend.block_cart_address_list"/>
            </div>
            <div class="col-12 col-md-6 col-lg-4 block block_cart_summary">

              <t t-set="checkout_button" t-value="False"/>
              <t t-set="cart" t-value="store['cart']"/>

              <t t-call="shopinvader_frontend.block_cart_summary">
                <t t-set="display_lines" t-value="True"/>
                <t t-set="submit_label" t-value="Continue"/>
                <t t-set="display_submit" t-value="True"/>
              </t>

            </div>
            <!-- {% global_section 'cart_breadcrumb' %} -->
            <!-- <t t-call="shopinvader_frontend.cart_breadcrumb"></t> -->
            <!-- <div class="pt-4 pb-4"> -->
            <!-- {% section 'cart' %} -->
            <!-- <t t-call="shopinvader_frontend.webshop_cart"></t> -->
            <!-- </div> -->
            <!-- {% sections_dropzone %} -->
            <!-- </div> -->
          </div>
          <!-- {% endblock %} -->
        </div>
      </div>
    </t>

  </template>

</odoo>

